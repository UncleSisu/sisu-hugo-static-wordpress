{
  "name": "hugo-starter",
  "version": "1.0.0",
  "description":
    "This is a boilerplate for using [Hugo](https://gohugo.io/) as a static site generator and Gulp + Webpack as your asset pipeline",
  "main": "index.js",
  "scripts": {
    "hugo": "gulp hugo",
    "serve": "node server.js",
    "build": "rm -rf ./dist && cross-env NODE_ENV=production gulp build",
    "build-preview":
      "rm -rf ./dist && cross-env NODE_ENV=production gulp build-preview",
    "dev": "rm -rf ./dist && gulp dev",
    "lint": "eslint src"
  },
  "author": "",
  "license": "MIT",
  "browserslist": ["> 1%", "last 2 versions"],
  "dependencies": {
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "jquery": "3.3.1",
    "jquery.easing": "^1.4.1",
    "lodash": "4.17.11",
    "modernizr": "3.6.0",
    "postal": "^2.0.5"
  },
  "devDependencies": {
    "autoprefixer": "9.3.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "8.0.4",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "6.26.0",
    "browser-sync": "2.26.3",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "exports-loader": "^0.6.3",
    "file-loader": "2.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "8.0.0",
    "gulp-cache-bust": "1.4.0",
    "gulp-notify": "3.2.0",
    "gulp-plumber": "1.2.0",
    "gulp-postcss": "8.0.0",
    "gulp-sass": "4.0.2",
    "gulp-sourcemaps": "2.6.4",
    "gulp-util": "^3.0.7",
    "imports-loader": "0.8.0",
    "modernizr-loader": "^1.0.1",
    "nodemon": "1.18.6",
    "run-sequence": "2.2.1",
    "url-loader": "1.1.2",
    "webpack": "^2.6.0",
    "yarn": "1.12.1"
  }
}
